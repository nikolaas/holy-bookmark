{
  "name": "social",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "debug": "better-npm-run debug"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "debug": {
      "command": "nodemon --inspect -w src --exec \"babel-node src --presets es2015,stage-0\"",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "babel src -s -D -d dist --presets es2015,stage-0",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleksey2stupak/social.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aleksey2stupak/social/issues"
  },
  "homepage": "https://github.com/aleksey2stupak/social#readme",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-session": "^1.15.6",
    "formidable": "^1.1.1",
    "mime": "^1.4.1",
    "morgan": "^1.8.0",
    "nconf": "^0.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "^0.1.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
